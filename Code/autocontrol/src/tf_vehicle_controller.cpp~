	//ende meine values

	
	photo_taken=false;vehicle_positioned=false;
	std::string new_listener_name;
	//std::string topic_name = argv[1];
	ros::init(argc, argv,"vehicle_tf_listener_UWVehicle");
	ros::NodeHandle n;
	image_transport::ImageTransport it(n);
	
	ros::Rate loop_rate(10);
	tf::TransformListener listener;

	//Publishers for Odometry and Joint space
	ros::Publisher odom_pub = n.advertise<nav_msgs::Odometry>("dataNavigator", 1);
	ros::Publisher js_pub = n.advertise<sensor_msgs::JointState>("/uwsim/joint_state_command_UWVehicle", 1);

	int first_pass;
	double Roll,Pitch,Yaw;
	float x_value,y_value,z_value,z_rot_value1,z_rot_value,endpart_rotate_axis_x,endpart_rotate_axis_y,endpart_rotate_axis_z;
	float moved_up,moved_down,j,moved_z,k,n_times;
	moved_up = 0;
	moved_down =10;
	moved_z=0;
	j=0;k=0;n_times=0;
	bool arm_extended,arm_retracted;
	arm_extended = false;
	arm_retracted=false;
	tf::Quaternion q;
	nav_msgs::Odometry odom;
	tf::StampedTransform transform_endpart,Pose_trans;

	x_value=1000;y_value=1000;z_value=1000;
	tf::StampedTransform transform_world;
	int movement_steps;
	movement_steps=0;
	float req_z;
	req_z=-10;
		
	//Subscriber of type image_tranport for camera topic from UWSim
	//image_transport::Subscriber sub1 = it.subscribe("uwsim/camera1", 1, imageCallback1);
	//image_transport::Subscriber sub2 = it.subscribe("uwsim/camera2", 1, imageCallback2);
	//image_transport::Subscriber sub3 = it.subscribe("uwsim/camera3", 1, imageCallback3);
	//image_transport::Subscriber sub4 = it.subscribe("uwsim/camera4", 1, imageCallback4);
	//image_transport::Subscriber sub5 = it.subscribe("uwsim/bottom_camera1", 1, imageCallback_bottom_camera);

	while (ros::ok())
	{
		sensor_msgs::JointState jointstatevalues;
		//Vehicle position is measured wrt the centre of Vehicle and rotated on the same as Axis. (wrt vehicle's center)
		try
		{	
			listener.waitForTransform("world","girona500/base_link",ros::Time(0),ros::Duration(10.0));
			listener.lookupTransform("world","girona500/base_link",ros::Time(0), transform_world);

			//listener.waitForTransform("world","UWVehicle/End_part",ros::Time(0),ros::Duration(10.0));
			//listener.lookupTransform("world","UWVehicle/End_part",ros::Time(0), transform_endpart);						
		}

		catch (tf::TransformException &ex) 
		{
			ROS_ERROR("%s",ex.what());
			ros::Duration(1.0).sleep();
		}

		x_value = transform_world.getOrigin().x();
		y_value = transform_world.getOrigin().y();
		z_value = transform_world.getOrigin().z();

		n.getParam("Vehicle_controller/Velocity",Vehicle_velocity);
		n.getParam("Vehicle_controller/first_pass",first_pass);

		std::cout<< "x value" << x_value << std::endl;
		std::cout<< "y value" << y_value << std::endl;
		std::cout<< "z value" << z_value << std::endl;

		//endpart_rotate_axis_x = transform_endpart.getOrigin().x();
		//endpart_rotate_axis_y = transform_endpart.getOrigin().y();
		//endpart_rotate_axis_z = transform_endpart.getOrigin().z();
		
		//Takes the panaromic view of the Coral the distance of -225 in X-direction
		//Normal movement -- Captures Image at a frequency 100 steps --> When boundary condition fails, the Vehicle rotates.
		if (first_time) 
			{
				std::cout<<"Inside 1st time"<<std::endl;	
				if(x_value >= 25) 
					{
						odom = rotate_vehicle(0,0,-90,odom,x_value,y_value,z_value,"UWVehicle_world");   
						second_time=true;first_time=false;
						//hunderedsecond_time=true;first_time=false;
						//seventyeighth_time=true;first_time=false;
					}
				else	
					{odom = apply_movement(Vehicle_velocity,odom);movement_steps+=1;req_z=-10;moved_z=0;movement_steps=0;arm_retracted=false;}
			}

		else if (second_time && first_pass!=1)
			{
				std::cout<<"Inside 2nd time"<<std::endl;
				if (y_value<=-0.5) 
					{
						odom = rotate_vehicle(0,0,180,odom,x_value,y_value,z_value,"UWVehicle_world");
							//endpart_rotate_axis_x,endpart_rotate_axis_y,endpart_rotate_axis_z,"UWVehicle_world");
						third_time = true; second_time=false;movement_steps=0;
					}
				else {odom = apply_movement(Vehicle_velocity,odom);}
			}

		else if (third_time && first_pass!=1)
			{
				std::cout<<"Inside 3rd time"<<std::endl;
				if (x_value<= -25) 
					{
						odom = rotate_vehicle(0,0,-90,odom,x_value,y_value,z_value,"UWVehicle_world");
						fourth_time = true; third_time=false;fifth_2ndtime=false;
					}
				else 
					{odom = apply_movement(Vehicle_velocity,odom);}
			}

		else if (fourth_time && first_pass!=1)
			{
				std::cout<<"Inside 4th time"<<std::endl;
				if (y_value <= -1) 
					{
						odom = rotate_vehicle(0,0,0,odom,x_value,y_value,z_value,"UWVehicle_world");
						fifth_time = true; fourth_time=false;movement_steps=0;
					}
				else {odom = apply_movement(Vehicle_velocity,odom);}
			}

		else if (fifth_time && first_pass!=1)
			{
				std::cout<<"Inside 5th time"<<std::endl;
				if (x_value >= 25) 
					{
						odom = rotate_vehicle(0,0,-90,odom,x_value,y_value,z_value,"UWVehicle_world");
						fifth_time = false; sixth_time=true; movement_steps=0;
					}
				else 
					{odom = apply_movement(Vehicle_velocity,odom);}
					
			}

